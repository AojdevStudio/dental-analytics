"temporal_context_calculations": {
  "week_position": {
    "formula": "current_day_of_week / 5",
    "interpretation": {
      "0.2": "Early week (Monday)",
      "0.4": "Early-mid week (Tuesday)",
      "0.6": "Mid-week (Wednesday)",
      "0.8": "Late week (Thursday)",
      "1.0": "Week complete (Friday)"
    }
  },
  "month_position": {
    "formula": "current_day_of_month / total_days_in_month",
    "interpretation": {
      "0-0.33": "Early month",
      "0.34-0.66": "Mid-month",
      "0.67-0.90": "Late month",
      "0.91-1.0": "Month-end"
    }
  },
  "days_remaining": {
    "weekly": "5 - current_day_of_week",
    "monthly": "total_days_in_month - current_day_of_month"
  },
  "progress_language": {
    "week": {
      "20%": "Just getting started this week",
      "40%": "Building momentum",
      "60%": "At the halfway point",
      "80%": "Final push",
      "100%": "Week complete"
    },
    "month": {
      "early": "Setting the pace for {month}",
      "mid": "Halfway through {month}",
      "late": "Closing out {month} strong",
      "end": "{month} complete"
    }
  }
},
