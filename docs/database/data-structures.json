"front_desk_calculations": {
  "hygiene_reappointment_rate": {
    "formula": "((total_hygiene_appointments - patients_not_reappointed) / total_hygiene_appointments) * 100",
    "format": "percentage",
    "success_threshold": 95,
    "description": "Target: 95%+ of hygiene patients reappointed"
  },
  "treatment_call_conversion_rate": {
    "formula": "(patients_scheduled_from_calls / unscheduled_tx_calls) * 100",
    "format": "percentage",
    "success_threshold": 30,
    "description": "Success metric: 30%+ conversion rate on treatment calls"
  },
  "recall_conversion_rate": {
    "formula": "(total_pts_scheduled / recalls_completed) * 100",
    "format": "percentage",
    "success_threshold": 25,
    "description": "Target: 25%+ of recall calls result in scheduled appointments"
  },
  "follow_up_conversion_rate": {
    "formula": "(patients_scheduled_from_follow_ups / follow_ups_completed_clickup) * 100",
    "format": "percentage",
    "success_threshold": 40,
    "description": "Target: 40%+ of follow-ups convert to appointments"
  },
  "case_acceptance_rate": {
    "formula": "(dollar_amount_scheduled / dollar_amount_presented) * 100",
    "format": "percentage",
    "success_threshold": 60,
    "description": "Target: 60%+ of presented treatment accepted"
  },
  "same_day_treatment_total": {
    "formula": "same_day_treatment",
    "format": "currency",
    "success_threshold": 1000,
    "description": "Target: $1,000+ same-day treatment accepted daily"
  },
  "cancellation_rate": {
    "formula": "(total_cancellations / total_scheduled_appointments) * 100",
    "format": "percentage"
  }
},
